{
  "mcpServers": {
    "frida": {
      "command": "bun",
      "args": [
        "run",
        "src/index.ts"
      ],
      "env": {
        "FRIDA_DEFAULT_DEVICE": "192.168.0.100:27042"
      },
      "disabled": false,
      "alwaysAllow": [
        "attach_to_process",
        "create_interactive_session",
        "execute_in_session",
        "load_script_file",
        "download_file"
      ]
    }
  }
}

